/* pages/activity/activity.wxss */

.lists .list image{
    width: 750rpx;
    height: 480rpx;
    position: relative;
    top: 0;
}
.list .title-bg {
    position: absolute;
    width: 100%;
    height: 328rpx;
    bottom: 0;
    background: url(data:../../image/png;base64,) no-repeat;
      background: -webkit-linear-gradient(rgba(0,0,0), rgba(0,0,0,0.65)); /* Safari 5.1 - 6.0 */
  background: -o-linear-gradient(rgba(0,0,0),rgba(0,0,0,0.65)); /* Opera 11.1 - 12.0 */
  background: -moz-linear-gradient(rgba(0,0,0), rgba(0,0,0,0.65)); /* Firefox 3.6 - 15 */
  background: linear-gradient(rgba(0,0,0), rgba(0,0,0,0.65)); /* 标准的语法 */
    /*background-image: url(../images/bottom_black_cover.png);
    background-repeat: repeat-x;*/
}
.list {
    width: 100%;
    height: 480rpx;
    overflow: hidden;
    position: relative;
    margin-bottom: 8rpx;
}
 .activity-info {
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 32rpx;
    box-sizing: border-box;
}
.list .activity-info .activity-type {
    margin: 0;
    width: 72rpx;
    height: 40rpx;
    border-radius: 6rpx;
    text-align: center;
    background-color: #ff5722;
    color: #fff;
    font-size: 24rpx;
    line-height: 40rpx;
    display: block;
}
.list .activity-info .title {
    display: inline-block;
    margin: 0;
    margin-top: 10rpx;
    font-size: 40rpx;
    font-weight: bold;
    color: #fff;
}
.list .activity-info .time-address {
    margin: 0;
    margin-top: 10rpx;
    font-size: 22rpx;
    /*font-weight: lighter;*/
    color: #e2e5d6;
}
.list .activity-info .time-address .time{
    margin-right: 10rpx;
}
.list .activity-info .location {
    position: absolute;
    right: 20rpx;
    bottom: 20rpx;
    width: 88rpx;
    overflow: hidden;
}
.main-container .location-icon {
    margin: 0;
}
location .location-icon img {
    width: 88rpx;
    height: 88rpx;
}
.list .activity-info .location .location-city {
    color: #fff;
    font-size: 22rpx;
    font-weight: lighter;
    text-align: center;
}